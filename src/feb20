class Solution {
    public int maximumGap(int[] nums) {
        Arrays.sort(nums);
        int maxdif=0;
        if(nums.length<2){
            return 0;
        }
        for(int i=1;i<nums.length;i++){
            maxdif=Math.max(maxdif,nums[i]-nums[i-1]);
        }
        return maxdif;
    }
}